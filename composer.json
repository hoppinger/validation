{
    "name": "hoppinger/validation-bundle",
    "type": "symfony-bundle",
    "description": " Bundle which adds several validator constraints to your Symfony project",
    "keywords": ["symfony2", "bundle", "validator", "constraint"],
    "homepage": "http://github.com/hoppinger/HopValidationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hoppinger BV",
            "email": "info@hoppinger.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/validator": ">=2.1,<2.3-dev",
        "symfony/form": ">=2.1,<2.3-dev",
        "symfony/framework-bundle": ">=2.1,<2.3-dev"
    },
    "autoload": {
        "psr-0": { "Hop\\ValidationBundle": "" }
    },
    "target-dir": "Hop/ValidationBundle"
}
